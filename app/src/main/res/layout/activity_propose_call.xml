<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.cationvideocall.example.captionvideocall.Activity.ProposeCallActivity">
        <ImageView
            android:id="@+id/imv_back"
            android:layout_width="39dp"
            android:layout_height="38dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/back" />

        <ImageView
            android:id="@+id/imv_logo"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="123dp"
            android:layout_height="108dp"
            android:layout_marginTop="56dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imv_logo"
            app:srcCompat="@drawable/profileimg" />

        <EditText
            android:id="@+id/et_callbookName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:focusableInTouchMode="false"
            android:text="막내 아들"
            android:textColor="#000000"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <TextView
            android:id="@+id/tv_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:text="아이디"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/et_counterid"
            app:layout_constraintEnd_toStartOf="@+id/et_counterid"
            app:layout_constraintTop_toTopOf="@+id/et_counterid" />

        <EditText
            android:id="@+id/et_counterid"
            android:layout_width="89dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:focusableInTouchMode="false"
            android:minHeight="48dp"
            android:text="test"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/et_callbookName"
            app:layout_constraintHorizontal_bias="0.545"
            app:layout_constraintStart_toStartOf="@+id/et_callbookName"
            app:layout_constraintTop_toBottomOf="@+id/et_callbookName" />

        <ImageView
            android:id="@+id/btn_call"
            android:layout_width="58dp"
            android:layout_height="56dp"
            android:layout_marginTop="644dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/callimg" />

        <ImageView
            android:id="@+id/imv_star"
            android:layout_width="46dp"
            android:layout_height="37dp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="@+id/et_counterid"
            app:layout_constraintStart_toEndOf="@+id/tv_id"
            app:layout_constraintTop_toBottomOf="@+id/et_counterid"
            app:srcCompat="@drawable/star2" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:text="즐겨찾기"
            app:layout_constraintBottom_toBottomOf="@+id/imv_star"
            app:layout_constraintEnd_toStartOf="@+id/imv_star"
            app:layout_constraintStart_toStartOf="@+id/tv_id"
            app:layout_constraintTop_toTopOf="@+id/imv_star" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="편집"
            app:layout_constraintEnd_toEndOf="@+id/ibtn_edit"
            app:layout_constraintStart_toStartOf="@+id/ibtn_edit"
            app:layout_constraintTop_toBottomOf="@+id/ibtn_edit" />

        <Button
            android:id="@+id/btn_saveedit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:backgroundTint="#EFFFFD"
            android:text="수정완료"
            android:textStyle="bold"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imv_logo" />

        <ImageButton
            android:id="@+id/ibtn_edit"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="28dp"
            android:backgroundTint="#EFFFFD"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/editpen" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
